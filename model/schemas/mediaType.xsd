				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="application/x-stuffit">
				<xs:annotation>
					<xs:documentation> StuffIt archive files</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="application/x-tar">
				<xs:annotation>
					<xs:documentation> Tarball files</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="application/x-www-form-urlencoded">
