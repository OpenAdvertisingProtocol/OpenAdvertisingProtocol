					<xs:documentation> Mozilla XUL files</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="application/vnd.google-earth.kml+xml">
				<xs:annotation>
					<xs:documentation> KML files (e.g. for Google Earth)[16]</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="application/vnd.google-earth.kmz">
				<xs:annotation>
					<xs:documentation> KMZ files (e.g. for Google Earth)[17]</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="application/vnd.dart">
				<xs:annotation>
					<xs:documentation> Dart files [18]</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="application/vnd.android.package-archive">
				<xs:annotation>
					<xs:documentation> For download apk files.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="application/vnd.ms-xpsdocument">
				<xs:annotation>
					<xs:documentation> XPS document[19]</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="application/x-7z-compressed">
				<xs:annotation>
					<xs:documentation> 7-Zip compression format.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="application/x-deb">
				<xs:annotation>
					<xs:documentation> deb (file format), a software package format used by the Debian project</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="application/x-dvi">
				<xs:annotation>
					<xs:documentation> device-independent document in DVI format</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="application/x-font-ttf">
				<xs:annotation>
					<xs:documentation> TrueType Font No registered MIME type, but this is the most commonly used</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="application/x-javascript">
				<xs:annotation>
					<xs:documentation/>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="application/x-latex">
				<xs:annotation>
					<xs:documentation> LaTeX files</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="application/x-mpegURL">
				<xs:annotation>
					<xs:documentation> .m3u8 variant playlist</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="application/x-rar-compressed">
				<xs:annotation>
					<xs:documentation> RAR archive files</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
