<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:vc="http://www.w3.org/2007/XMLSchema-versioning" elementFormDefault="qualified" attributeFormDefault="unqualified" vc:minVersion="1.1">
	<xs:simpleType name="MediaType">
		<xs:annotation>
			<xs:documentation>IANA-standard Internet Media Types</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="image/gif">
				<xs:annotation>
					<xs:documentation> GIF image; Defined in RFC 2045 and RFC 2046</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="image/jpeg">
				<xs:annotation>
					<xs:documentation> JPEG JFIF image; Defined in RFC 2045 and RFC 2046</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="image/pjpeg">
				<xs:annotation>
					<xs:documentation> JPEG JFIF image; Associated with Internet Explorer; Listed in ms775147(v=vs.85) - Progressive JPEG, initiated before global browser support for progressive JPEGs (Microsoft and Firefox).</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="image/png">
				<xs:annotation>
					<xs:documentation> Portable Network Graphics; Registered,[10] Defined in RFC 2083</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="image/svg+xml">
				<xs:annotation>
					<xs:documentation> SVG vector image; Defined in SVG Tiny 1.2 Specification Appendix M</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="image/tiff">
				<xs:annotation>
					<xs:documentation> Tag Image File Format (only for Baseline TIFF); Defined in RFC 3302</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="audio/basic">
				<xs:annotation>
					<xs:documentation> μ-law audio at 8 kHz, 1 channel; Defined in RFC 2046</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="audio/L24">
				<xs:annotation>
					<xs:documentation> 24bit Linear PCM audio at 8–48 kHz, 1-N channels; Defined in RFC 3190</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="audio/mp4">
				<xs:annotation>
					<xs:documentation> MP4 audio</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="audio/mpeg">
				<xs:annotation>
					<xs:documentation> MP3 or other MPEG audio; Defined in RFC 3003</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="audio/ogg">
				<xs:annotation>
					<xs:documentation> Ogg Vorbis, Speex, Flac and other audio; Defined in RFC 5334</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="audio/vorbis">
				<xs:annotation>
					<xs:documentation> Vorbis encoded audio; Defined in RFC 5215</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="audio/vnd.rn-realaudio">
				<xs:annotation>
					<xs:documentation> RealAudio; Documented in RealPlayer Help[9]</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="audio/vnd.wave">
				<xs:annotation>
					<xs:documentation> WAV audio; Defined in RFC 2361</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="audio/webm">
				<xs:annotation>
					<xs:documentation> WebM open media format</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="video/mpeg">
				<xs:annotation>
					<xs:documentation> MPEG-1 video with multiplexed audio; Defined in RFC 2045 and RFC 2046</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="video/mp4">
				<xs:annotation>
					<xs:documentation> MP4 video; Defined in RFC 4337</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="video/ogg">
				<xs:annotation>
					<xs:documentation> Ogg Theora or other video (with audio); Defined in RFC 5334</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="video/quicktime">
				<xs:annotation>
					<xs:documentation> QuickTime video; Registered[11]</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="video/webm">
				<xs:annotation>
					<xs:documentation> WebM Matroska-based open media format</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="video/x-matroska">
				<xs:annotation>
					<xs:documentation> Matroska open media format</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="video/x-ms-wmv">
				<xs:annotation>
					<xs:documentation> Windows Media Video; Documented in Microsoft KB 288102</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="video/x-flv">
				<xs:annotation>
					<xs:documentation> Flash video (FLV files)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="application/atom+xml">
				<xs:annotation>
					<xs:documentation> Atom feeds</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="application/ecmascript">
				<xs:annotation>
					<xs:documentation> ECMAScript/JavaScript; Defined in RFC 4329 (equivalent to application/javascript but with stricter processing rules)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="application/EDI-X12">
				<xs:annotation>
					<xs:documentation> EDI X12 data; Defined in RFC 1767</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="application/EDIFACT">
				<xs:annotation>
					<xs:documentation> EDI EDIFACT data; Defined in RFC 1767</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="application/json">
				<xs:annotation>
					<xs:documentation> JavaScript Object Notation JSON; Defined in RFC 4627</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="application/javascript">
				<xs:annotation>
					<xs:documentation> ECMAScript/JavaScript; Defined in RFC 4329 (equivalent to application/ecmascript but with looser processing rules) It is not accepted in IE 8 or earlier - text/javascript is accepted but it is defined as obsolete in RFC 4329. The "type" attribute of the script tag in HTML5 is optional. In practice, omitting the media type of JavaScript programs is the most interoperable solution, since all browsers have always assumed the correct default even before HTML5.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="application/octet-stream">
				<xs:annotation>
					<xs:documentation> Arbitrary binary data.[6] Generally speaking this type identifies files that are not associated with a specific application. Contrary to past assumptions by software packages such as Apache this is not a type that should be applied to unknown files. In such a case, a server or application should not indicate a content type, as it may be incorrect, but rather, should omit the type in order to allow the recipient to guess the type.[7]</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="application/ogg">
				<xs:annotation>
					<xs:documentation> Ogg, a multimedia bitstream container format; Defined in RFC 5334</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="application/pdf">
				<xs:annotation>
					<xs:documentation> Portable Document Format, PDF has been in use for document exchange on the Internet since 1993; Defined in RFC 3778</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="application/postscript">
				<xs:annotation>
					<xs:documentation> PostScript; Defined in RFC 2046</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="application/rdf+xml">
				<xs:annotation>
					<xs:documentation> Resource Description Framework; Defined by RFC 3870</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="application/rss+xml">
				<xs:annotation>
					<xs:documentation> RSS feeds</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="application/soap+xml">
				<xs:annotation>
					<xs:documentation> SOAP; Defined by RFC 3902</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="application/font-woff">
				<xs:annotation>
					<xs:documentation> Web Open Font Format; (candidate recommendation; use application/x-font-woff until standard is official)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="application/xhtml+xml">
				<xs:annotation>
					<xs:documentation> XHTML; Defined by RFC 3236</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="application/xml">
				<xs:annotation>
					<xs:documentation> XML files; Defined by RFC 3023</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="application/xml-dtd">
				<xs:annotation>
					<xs:documentation> DTD files; Defined by RFC 3023</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="application/xop+xml">
				<xs:annotation>
					<xs:documentation> XOP</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="application/zip">
				<xs:annotation>
					<xs:documentation> ZIP archive files; Registered[8]</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="application/gzip">
				<xs:annotation>
					<xs:documentation> Gzip, Defined in RFC 6713</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="message/http">
				<xs:annotation>
					<xs:documentation> Defined in RFC 2616</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="message/imdn+xml">
